@page
@model Veterinaria.App.Presentacion.Pages.AdminVeterinarioModel
@{
}

<h1>Diligencia los datos del Administrador</h1>
<form>
    <div class="form-group">
        <label for="id-admin-veterinario-nombre">Nombre</label>
        <input type="string" class="form-control" id="id-admin-veterinario-nombre" aria-describedby="emailHelp">
        <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
    </div>
    <div class="mb-3">
        <label for="exampleInputPassword1" class="form-label">Password</label>
        <input type="password" class="form-control" id="exampleInputPassword1">
    </div>
    <div class="mb-3 form-check">
        <input type="checkbox" class="form-check-input" id="exampleCheck1">
        <label class="form-check-label" for="exampleCheck1">Check me out</label>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>